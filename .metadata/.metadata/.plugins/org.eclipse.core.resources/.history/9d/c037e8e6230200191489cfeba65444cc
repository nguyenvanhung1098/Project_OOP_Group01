package create;

public class QuanHe {
	private String quanhe; 
	public Country country;
	public Event event;
	public Location location;
	public Organization organization;
	public Person person;
	public QuanHe() {		
	}
	
//	public QuanHe(Location location, Country country, String quanhe) {
//		this.location = location;
//		this.country = country;
//		this.quanhe= quanhe;
//	}
	
	public QuanHe(Person person , Organization organization, String quanhe) {
		this.person = person;
		this.organization= organization;
		this.quanhe = quanhe;
	}
	
	public QuanHe(Event event, Country country, String quanhe) {
		this.event= event;
		this.country = country;
		this.quanhe = quanhe;
	}
	
	public QuanHe(Event event, Location location , String quanhe) {
		this.event= event;
		this.location = location;
		this.quanhe = quanhe;
	}
	
	public QuanHe(Person person, Country country, String quanhe) {
		this.person = person;
		this.country = country;
		this.quanhe = quanhe;
	}
	
	public QuanHe(Organization organization, Country country, String quanhe) {
		this.organization = organization;
		this.country = country;
		this.quanhe = quanhe;
	}
	
	public QuanHe(Organization organization, Location location, String quanhe) {
		this.organization = organization;
		this.location = location;
		this.quanhe = quanhe;
	}
	public String getQuanhe() {
		return quanhe;
	}
	public void setQuanhe(String quanhe) {
		this.quanhe = quanhe;
	}
	
	
}
